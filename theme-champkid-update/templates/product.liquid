{% assign bodyClasses = "" %}

{% render 'navigation' %}

<div id="shopify-section-product" class="shopify-section">
  <div data-section-id="{{ section.id }}" data-section-type="product-template">
    {% section 'product-hero' %}
    
    {% section 'product-gallery' %}

    {% section 'product-details' %}}
    
    <!-- App blocks section 2 - For subscription widgets, forms, etc. -->
    {% section 'product-form' %}
    
    {% section 'product-purchase' %}
    
    {% section 'product-experience' %}
    
    {% section 'product-faq' %}
    
    {% section 'product-recommended' %}
    
    {% render 'product-schema' %}
  </div>
</div>

{% render 'footer' %}

<img src="{{ 'gradient-circle.avif' | asset_url }}" loading="lazy" sizes="(max-width: 1806px) 100vw, 1806px" srcset="{{ 'gradient-circle-p-500.png' | asset_url }} 500w, {{ 'gradient-circle-p-800.png' | asset_url }} 800w, {{ 'gradient-circle-p-1080.png' | asset_url }} 1080w, {{ 'gradient-circle.avif' | asset_url }} 1806w" alt="" class="bg-blob product">

